<Application
    x:Class="PasswordSaver.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:PasswordSaver"
    RequestedTheme="Light"
    >
    <Application.Resources>
        <local:CollapseBindingHelper x:Key="collapseConverter"/>
        <local:VisibleBindingHelper x:Key="visibleConverter"/>
        <local:FontSizeBindingHelper x:Key="fontsizeConverter"/>
        <local:StringVisibleBindingHelper x:Key="stringVisibleConverter"/>
    </Application.Resources>
</Application>
